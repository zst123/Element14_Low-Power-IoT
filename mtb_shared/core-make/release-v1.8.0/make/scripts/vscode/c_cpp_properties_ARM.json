{
    "version": 4,
    "env": {
        "mtbCompilerArgs": [
            "&&CY_C_FLAGS&&"
        ],
        "mtbDefaultIncludePath": [
            &&CY_INCLUDE_LIST&&
            "&&CY_TOOL_CHAIN_DIRECTORY&&/include/**"
        ],
        "mtbDefaultDefines": [
            &&CY_DEFINE_LIST&&
            "__GNUC__"
        ]
    },
    "configurations": [
        {
            "name": "MTB",
            "compilerPath": "&&CY_TOOL_CHAIN_DIRECTORY&&/bin/armclang",
            "compilerArgs": [
                "${mtbCompilerArgs}"
            ],
            "compileCommands": "&&CY_CDB_FILE&&",
            "intelliSenseMode": "gcc-x86",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "includePath": [
                "${mtbDefaultIncludePath}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "defines": [
                "${mtbDefaultDefines}"
            ]
        }
    ]
}
